<h2> Edit Player</h2>
<%= form_for([@category,@product],:remote => :true,  :method => "PUT") do |f| %>
<div class="form-field">
<%= f.label :name %>
<%= f.text_field :name %>
</div>


<div class="form-field">
<%= f.label :description %>
<%= f.text_field :description %>
</div>
<div class="form-field">
<%= f.label :price %>
<%= f.text_field :price %>
</div>
<%= f.submit%>

<%end%>

